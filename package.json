{
  "name": "loopback-api-base",
  "docker_container_name": "api",
  "version": "0.0.1",
  "main": "server/server.js",
  "engines": {
    "node": "8.10.0"
  },
  "scripts": {
    "lint": "eslint .",
    "dev": "nodemon --exec node server/server.js",
    "prod": "node server/server.js",
    "pretest": "npm run autoupdate-test",
    "test": "NODE_ENV=test istanbul cover _mocha $(find -name \"*.spec.js\" -not -path \"./node_modules/*\")",
    "posttest": "npm run integration-test",
    "preunit-test": "npm run autoupdate-test",
    "unit-test": "NODE_ENV=test istanbul cover _mocha $(find -name \"*.spec.js\" -not -path \"./node_modules/*\")",
    "postunit-test": "npm run lint && nsp check",
    "preintegration-test": "npm run autoupdate-test",
    "integration-test": "NODE_ENV=test mocha $(find -name \"*.test.js\" -not -path \"./node_modules/*\")",
    "postintegration-test": "npm run lint && nsp check",
    "precommit": "docker-compose run --rm api npm run test",
    "console": "loopback-console",
    "autoupdate": "node server/autoupdate.js",
    "autoupdate-test": "NODE_ENV=test node server/autoupdate.js",
    "postautoupdate-test": "NODE_ENV=test node server/db_migrate.js",
    "postautoupdate": "node server/db_migrate.js",
    "dbMigrate": "node server/db_migrate.js",
    "migrationGenerator": "node server/generators/migrationGenerator.js",
    "validate": "npm ls"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "chai-as-promised": "^7.1.1",
    "compression": "^1.0.3",
    "cors": "^2.8.1",
    "google-geocoder": "^0.2.1",
    "image-downloader": "^3.3.0",
    "lodash": "^4.17.5",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.4.0",
    "loopback-connector-postgresql": "^2.8.0",
    "loopback-console": "^1.1.0",
    "loopback-context": "^3.3.0",
    "loopback-setup-remote-methods-mixin": "^1.0.4",
    "mime": "2.0.3",
    "moment": "^2.22.1",
    "nodemon": "^1.17.1",
    "serve-favicon": "^2.0.1",
    "sharp": "^0.20.1",
    "shelljs": "^0.8.1",
    "strong-error-handler": "^1.1.1",
    "supertest": "^3.0.0",
    "underscore-node": "^0.1.2",
    "uuid": "^3.2.1",
    "validator": "^9.4.1",
    "webpack": "^4.1.0",
    "winston": "^2.4.1",
    "xlsx": "^0.12.6"
  },
  "optionalDependencies": {
    "loopback-component-explorer": "^2.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.9.0",
    "factory-girl": "^5.0.2",
    "faker": "^4.1.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-standard": "^1.6.6",
    "eslint-config-loopback": "^8.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "istanbul": "1.0.0-alpha.1",
    "mocha": "^5.0.5",
    "nsp": "^2.1.0",
    "sinon": "^4.4.8",
    "superagent": "3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajaykkumarrow/loopback-api-base.git"
  },
  "license": "MIT",
  "description": "loopback-api-base"
}
